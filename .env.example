# UiPath Configuration (Vite uses VITE_ prefix)
# Copy this file to .env and fill in your actual values

# UiPath Cloud URL (e.g., https://cloud.uipath.com, https://staging.uipath.com)
VITE_UIPATH_BASE_URL=https://staging.uipath.com

# UiPath JWT Secret Token (Bearer token from UiPath Cloud)
VITE_UIPATH_SECRET=your_jwt_token_here

# UiPath Organization Name
VITE_UIPATH_ORG_NAME=your_org_name

# UiPath Tenant Name
VITE_UIPATH_TENANT_NAME=your_tenant_name

# Development Environment
NODE_ENV=development